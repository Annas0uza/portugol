Algoritmo "crde"


Var

   reais, dolares, euro : real


Inicio


      escreva("Quantos reais você tem? ")
      leia(reais)
      
      dolares <- reais/4.62
      euro <- reais/5.01
      
      escreva("Em dolar você tem: ", dolares:2:2, " e em euro você tem: ", euro:2:2)



Fimalgoritmo