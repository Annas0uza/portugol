Algoritmo "teste_saida"

Var

   nome : caractere
   idade : inteiro
   salario : real


Inicio

      nome <- "Maria da Silva"
      idade <- 30
      salario <- 500.25
      
      escreval("A funcionaria ", nome, " tem ", idade, " anos ", " e recebe ", salario, " de salario ")

Fimalgoritmo