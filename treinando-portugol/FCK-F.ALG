Algoritmo "FCK-F"

Var

   f, c, k : real



Inicio

       escreva("Informe a temperatura em graus fahreinth: ")

      leia(f)



      c <- (f - 32) / 1.8

      k <- (f + 459.67) * (5 / 9)



      escreval("A temperatura em celsius é: ", c)

      escreval("A temperatura em kelvin é: ", k)



Fimalgoritmo